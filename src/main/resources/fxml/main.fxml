<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="root" prefHeight="750.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainController">
    <children>
        <MenuBar prefHeight="25.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <menus>
                <Menu mnemonicParsing="false" text="文件">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#fileLoad" text="加载"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="编译">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#compile1" text="词法分析"/>
                        <MenuItem mnemonicParsing="false" onAction="#compile2" text="语法分析"/>
                        <MenuItem mnemonicParsing="false" onAction="#compile3" text="语义分析"/>
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="更多">
                    <items>
                        <MenuItem mnemonicParsing="false" onAction="#About" text="About"/>
                    </items>
                </Menu>
            </menus>
        </MenuBar>

    </children>
</AnchorPane>
